{
  "name": "@nithish6112/mcp-inspector",
  "version": "0.1.0",
  "description": "A modern Model Context Protocol (MCP) client with beautiful UI, HTTP logging, and OAuth 2.1 support",
  "type": "module",
  "bin": {
    "mcp-client": "./bin/cli.js"
  },
  "main": "dist/server/index.js",
  "files": [
    "bin/",
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run dev --workspace=server",
    "dev:client": "npm run dev --workspace=client",
    "build": "npm run build:server && npm run build:client && npm run copy-client",
    "build:server": "npm run build --workspace=server",
    "build:client": "npm run build --workspace=client",
    "copy-client": "node scripts/copy-client.js",
    "start": "node dist/server/index.js",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "anthropic",
    "claude",
    "ai",
    "llm",
    "oauth",
    "inspector",
    "client",
    "debugging"
  ],
  "author": "Aquera Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nithish6112/mcp-inspector.git"
  },
  "bugs": {
    "url": "https://github.com/nithish6112/mcp-inspector/issues"
  },
  "homepage": "https://github.com/nithish6112/mcp-inspector#readme",
  "workspaces": [
    "client",
    "server"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  }
}